<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude sensitive data from cloud backups (Android 12+) -->
        <exclude domain="sharedpref" path="neptun_presence.xml" />
    </cloud-backup>
    
    <device-transfer>
        <!-- Include app data for device-to-device transfer -->
        <include domain="sharedpref" path="." />
    </device-transfer>
</data-extraction-rules>
